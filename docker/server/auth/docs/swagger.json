{
    "swagger": "2.0",
    "info": {
        "description": "This server is designed to provide jwt token support/local file handling to my gbajs implementation",
        "title": "Gba File/JWT Auth Server",
        "contact": {
            "name": "Nicholas VanCise",
            "url": "https://nicholas-vancise.dev",
            "email": "nvancise@unlv.nevada.edu"
        },
        "license": {
            "name": "Copyright (c) 2022 Nicholas VanCise"
        },
        "version": "1.0"
    },
    "host": "{{.Host}}",
    "basePath": "{{.BasePath}}",
    "paths": {
        "/": {
            "get": {
                "description": "Displays welcome message.",
                "produces": [
                    "text/plain"
                ],
                "summary": "Hello world landing page",
                "parameters": [
                    {
                        "type": "string",
                        "description": "Authorization",
                        "name": "authToken",
                        "in": "header",
                        "required": true
                    }
                ],
                "responses": {
                    "200": {
                        "description": "Hello World! This is a GBA file/auth server, written in Golang.",
                        "schema": {
                            "type": "string"
                        }
                    },
                    "401": {
                        "description": "Unauthorized",
                        "schema": {
                            "type": "string"
                        }
                    },
                    "405": {
                        "description": "Method Not Allowed",
                        "schema": {
                            "type": "string"
                        }
                    },
                    "500": {
                        "description": "Internal Server Error",
                        "schema": {
                            "type": "string"
                        }
                    },
                    "501": {
                        "description": "Not Implemented",
                        "schema": {
                            "type": "string"
                        }
                    }
                }
            }
        },
        "/api-v1/accounts/login": {
            "post": {
                "description": "User login from credentials, issues refresh token cookie, and access token",
                "consumes": [
                    "application/json"
                ],
                "tags": [
                    "auth"
                ],
                "summary": "User Login",
                "parameters": [
                    {
                        "description": "User credentials",
                        "name": "Data",
                        "in": "body",
                        "required": true,
                        "schema": {
                            "type": "object",
                            "$ref": "#/definitions/string"
                        }
                    }
                ],
                "responses": {
                    "200": {
                        "description": "Access token",
                        "schema": {
                            "type": "string"
                        }
                    },
                    "400": {
                        "description": "Bad Request",
                        "schema": {
                            "type": "string"
                        }
                    },
                    "401": {
                        "description": "Unauthorized",
                        "schema": {
                            "type": "string"
                        }
                    },
                    "405": {
                        "description": "Method Not Allowed",
                        "schema": {
                            "type": "string"
                        }
                    },
                    "500": {
                        "description": "Internal Server Error",
                        "schema": {
                            "type": "string"
                        }
                    }
                }
            }
        },
        "/api-v1/accounts/logout": {
            "post": {
                "description": "User logout",
                "tags": [
                    "auth"
                ],
                "summary": "User Logout",
                "parameters": [
                    {
                        "type": "string",
                        "description": "Bearer \u003ctoken\u003e",
                        "name": "Authorization",
                        "in": "header",
                        "required": true
                    }
                ],
                "responses": {
                    "200": {
                        "description": "OK",
                        "schema": {
                            "type": "string"
                        }
                    },
                    "401": {
                        "description": "Unauthorized",
                        "schema": {
                            "type": "string"
                        }
                    },
                    "405": {
                        "description": "Method Not Allowed",
                        "schema": {
                            "type": "string"
                        }
                    },
                    "500": {
                        "description": "Internal Server Error",
                        "schema": {
                            "type": "string"
                        }
                    }
                }
            }
        },
        "/api-v1/tokens/refresh": {
            "post": {
                "description": "Uses stored refresh token + database lookup to issue new auth token",
                "produces": [
                    "application/json"
                ],
                "tags": [
                    "auth"
                ],
                "summary": "Uses stored refresh token cookie to issue new auth token",
                "responses": {
                    "200": {
                        "description": "Access token",
                        "schema": {
                            "type": "string"
                        }
                    },
                    "401": {
                        "description": "Unauthorized",
                        "schema": {
                            "type": "string"
                        }
                    },
                    "405": {
                        "description": "Method Not Allowed",
                        "schema": {
                            "type": "string"
                        }
                    },
                    "500": {
                        "description": "Internal Server Error",
                        "schema": {
                            "type": "string"
                        }
                    }
                }
            }
        },
        "/api/rom/download": {
            "get": {
                "description": "Download rom from server",
                "produces": [
                    "application/x-gba-rom"
                ],
                "tags": [
                    "gba"
                ],
                "summary": "Download rom from server",
                "parameters": [
                    {
                        "type": "string",
                        "description": "Authorization",
                        "name": "authToken",
                        "in": "header",
                        "required": true
                    },
                    {
                        "type": "string",
                        "description": "Rom to download",
                        "name": "rom",
                        "in": "query",
                        "required": true
                    }
                ],
                "responses": {
                    "200": {
                        "description": "OK",
                        "schema": {
                            "type": "string"
                        }
                    },
                    "401": {
                        "description": "Unauthorized",
                        "schema": {
                            "type": "string"
                        }
                    },
                    "405": {
                        "description": "Method Not Allowed",
                        "schema": {
                            "type": "string"
                        }
                    },
                    "500": {
                        "description": "Internal Server Error",
                        "schema": {
                            "type": "string"
                        }
                    },
                    "501": {
                        "description": "Not Implemented",
                        "schema": {
                            "type": "string"
                        }
                    }
                }
            }
        },
        "/api/rom/list": {
            "get": {
                "description": "Lists all roms uploaded to server",
                "produces": [
                    "application/json"
                ],
                "tags": [
                    "gba"
                ],
                "summary": "Lists all rom files uploaded to server",
                "parameters": [
                    {
                        "type": "string",
                        "description": "Authorization",
                        "name": "authToken",
                        "in": "header",
                        "required": true
                    }
                ],
                "responses": {
                    "200": {
                        "description": "OK",
                        "schema": {
                            "type": "string"
                        }
                    },
                    "401": {
                        "description": "Unauthorized",
                        "schema": {
                            "type": "string"
                        }
                    },
                    "405": {
                        "description": "Method Not Allowed",
                        "schema": {
                            "type": "string"
                        }
                    },
                    "500": {
                        "description": "Internal Server Error",
                        "schema": {
                            "type": "string"
                        }
                    },
                    "501": {
                        "description": "Not Implemented",
                        "schema": {
                            "type": "string"
                        }
                    }
                }
            }
        },
        "/api/rom/upload": {
            "post": {
                "description": "Upload rom to server",
                "tags": [
                    "gba"
                ],
                "summary": "Upload rom to server",
                "parameters": [
                    {
                        "type": "string",
                        "description": "Authorization",
                        "name": "authToken",
                        "in": "header",
                        "required": true
                    },
                    {
                        "type": "file",
                        "description": "Rom to Upload",
                        "name": "rom",
                        "in": "formData",
                        "required": true
                    }
                ],
                "responses": {
                    "200": {
                        "description": "OK",
                        "schema": {
                            "type": "string"
                        }
                    },
                    "401": {
                        "description": "Unauthorized",
                        "schema": {
                            "type": "string"
                        }
                    },
                    "405": {
                        "description": "Method Not Allowed",
                        "schema": {
                            "type": "string"
                        }
                    },
                    "500": {
                        "description": "Internal Server Error",
                        "schema": {
                            "type": "string"
                        }
                    },
                    "501": {
                        "description": "Not Implemented",
                        "schema": {
                            "type": "string"
                        }
                    }
                }
            }
        },
        "/api/save/download": {
            "get": {
                "description": "Download save from server",
                "produces": [
                    "application/x-spss-sav"
                ],
                "tags": [
                    "gba"
                ],
                "summary": "Download save from server",
                "parameters": [
                    {
                        "type": "string",
